%==============================================================================%
% Intégration avec UPSTI_Document
%==============================================================================%
% On trouve la filiere en fonction de la classe, si ladite classe est déjà définie:
% 1- PTSI/PT
% 2- PCSI/PSI (PC ?)
% 3- MPSI/MP
% 4- TSI
% 5- ATS
% 6- MP2I/PSI
% Par défaut, on spécifie PCSI/PSI (cas le plus probable de document commun entre PCSI et MPSI)

% Equivalence UPSTIidClasse - UPSTIidFiliere
% 1-PTSI, 2-PT, 3-PT* => 1
% 4-PCSI, 5-PSI, 6-PSI*, 13-PC*, 14-PC, 15-Sup ou 16-Spe => 2
% 7-MPSI, 8-MP, 9-MP* => 3
% 10-TSI1 ou 11-TSI2 ou 19-TSI0=> 4
% 12-ATS => 5
% 17-MP2I, 18-MPI => 6

\newcommand{\UPSTIdetermineIdFiliere}{

  % Si la filière n'est pas définie, on se place en PTSI/PT par défaut
  \ifdef{\UPSTIidFiliere}{}{\newcommand{\UPSTIidFiliere}{1}}

  % Si on ne connait que l'idClasse, on en deduit l'idFiliere (idClasse est prioritaire par rapport à idFiliere)
  \ifdef{\UPSTIidClasse}{
    \ifcase\UPSTIidClasse
      Erreur dans UPSTIidClasse (=0)
      \or % PTSI
        \ifdef{\UPSTIidFiliere}{\renewcommand{\UPSTIidFiliere}{1}}{\newcommand{\UPSTIidFiliere}{1}}
      \or % PT
        \ifdef{\UPSTIidFiliere}{\renewcommand{\UPSTIidFiliere}{1}}{\newcommand{\UPSTIidFiliere}{1}}
      \or % PT*
        \ifdef{\UPSTIidFiliere}{\renewcommand{\UPSTIidFiliere}{1}}{\newcommand{\UPSTIidFiliere}{1}}
      \or % PCSI
        \ifdef{\UPSTIidFiliere}{\renewcommand{\UPSTIidFiliere}{2}}{\newcommand{\UPSTIidFiliere}{2}}
      \or % PSI
        \ifdef{\UPSTIidFiliere}{\renewcommand{\UPSTIidFiliere}{2}}{\newcommand{\UPSTIidFiliere}{2}}
      \or % PSI*
        \ifdef{\UPSTIidFiliere}{\renewcommand{\UPSTIidFiliere}{2}}{\newcommand{\UPSTIidFiliere}{2}}
      \or % MPSI
        \ifdef{\UPSTIidFiliere}{\renewcommand{\UPSTIidFiliere}{3}}{\newcommand{\UPSTIidFiliere}{3}}
      \or % MP
        \ifdef{\UPSTIidFiliere}{\renewcommand{\UPSTIidFiliere}{3}}{\newcommand{\UPSTIidFiliere}{3}}
      \or % MP*
        \ifdef{\UPSTIidFiliere}{\renewcommand{\UPSTIidFiliere}{3}}{\newcommand{\UPSTIidFiliere}{3}}
      \or % TSI1
        \ifdef{\UPSTIidFiliere}{\renewcommand{\UPSTIidFiliere}{4}}{\newcommand{\UPSTIidFiliere}{4}}
      \or % TSI2
        \ifdef{\UPSTIidFiliere}{\renewcommand{\UPSTIidFiliere}{4}}{\newcommand{\UPSTIidFiliere}{4}}
      \or % ATS
        \ifdef{\UPSTIidFiliere}{\renewcommand{\UPSTIidFiliere}{5}}{\newcommand{\UPSTIidFiliere}{5}}
      \or % PC
        \ifdef{\UPSTIidFiliere}{\renewcommand{\UPSTIidFiliere}{2}}{\newcommand{\UPSTIidFiliere}{2}}
      \or % PC*
        \ifdef{\UPSTIidFiliere}{\renewcommand{\UPSTIidFiliere}{2}}{\newcommand{\UPSTIidFiliere}{2}}
      \or % Sup
        \ifdef{\UPSTIidFiliere}{\renewcommand{\UPSTIidFiliere}{2}}{\newcommand{\UPSTIidFiliere}{2}}
      \or % Spé
        \ifdef{\UPSTIidFiliere}{\renewcommand{\UPSTIidFiliere}{2}}{\newcommand{\UPSTIidFiliere}{2}}
      \or % MP2I
        \ifdef{\UPSTIidFiliere}{\renewcommand{\UPSTIidFiliere}{6}}{\newcommand{\UPSTIidFiliere}{6}}
      \or % MPI
        \ifdef{\UPSTIidFiliere}{\renewcommand{\UPSTIidFiliere}{6}}{\newcommand{\UPSTIidFiliere}{6}}
      \or % MPI*
        \ifdef{\UPSTIidFiliere}{\renewcommand{\UPSTIidFiliere}{6}}{\newcommand{\UPSTIidFiliere}{6}}
      \or % TSI0
        \ifdef{\UPSTIidFiliere}{\renewcommand{\UPSTIidFiliere}{4}}{\newcommand{\UPSTIidFiliere}{4}}
      \else Erreur dans UPSTIidClasse (Trop grand)
    \fi
  }{}
}


