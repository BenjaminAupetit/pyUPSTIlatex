% Diagramme des compétences
%--------------------------------------------------------%
% Gestion des actifs/inactifs
\newcommand*{\UPSTIcompetenceActifInactif}[1]{\ifnumequal{0}{#1}{inactif}{actif}}

% Personnalisation
\newcommand*{\UPSTIcompetencesRayonEllipse}{1.9}

% #1: echelle, #2: filiere*, #3-9: activation ou non des compétences (activées par défaut)
% 1- PTSI/PT
% 2- PCSI/PSI (PC ?)
% 3- MPSI/MP
% 4- TSI
% 5- ATS
% 6- MP2I/PSI
\newcommandx{\UPSTIdiagrammeCompetences}[9][1=,2=,3=1,4=1,5=1,6=1,7=1,8=1,9=1]	%
{
  % Intégration avec UPSTI_Document
  \UPSTIdetermineIdFiliere

  % Filiere: le 2ème paramètre facultatif écrase idFiliere
  \ifstrempty{#2}
  {
    \ifdef{\UPSTIidFiliereTmp}{\renewcommand{\UPSTIidFiliereTmp}{\UPSTIidFiliere}}{\newcommand{\UPSTIidFiliereTmp}{\UPSTIidFiliere}}
  }{
    \ifdef{\UPSTIidFiliereTmp}{\renewcommand{\UPSTIidFiliereTmp}{#2}}{\newcommand{\UPSTIidFiliereTmp}{#2}}
  }

  % Un switch pour "fusionner" les filières qui ont le même diagramme de compétence: (ex: PTSI/PT-TSI-ATS)
  \ifcase\UPSTIidFiliereTmp
    Erreur dans UPSTIidFiliere (=0)
    \or %PTSI/PT
      \ifdef{\UPSTIidFilierePourDiagrammeCompetences}{\renewcommand{\UPSTIidFilierePourDiagrammeCompetences}{0}}{\newcommand{\UPSTIidFilierePourDiagrammeCompetences}{0}}
      \ifdef{\UPSTItitreClasseComp}{\renewcommand*{\UPSTItitreClasseComp}{PTSI-PT}}{\newcommand*{\UPSTItitreClasseComp}{PTSI-PT}}
    \or %PCSI/PSI
      \ifdef{\UPSTIidFilierePourDiagrammeCompetences}{\renewcommand{\UPSTIidFilierePourDiagrammeCompetences}{1}}{\newcommand{\UPSTIidFilierePourDiagrammeCompetences}{1}}
      \ifdef{\UPSTItitreClasseComp}{\renewcommand*{\UPSTItitreClasseComp}{PCSI-PSI}}{\newcommand*{\UPSTItitreClasseComp}{PCSI-PSI}}
    \or %MPSI/MP
      \ifdef{\UPSTIidFilierePourDiagrammeCompetences}{\renewcommand{\UPSTIidFilierePourDiagrammeCompetences}{2}}{\newcommand{\UPSTIidFilierePourDiagrammeCompetences}{2}}
      \ifdef{\UPSTItitreClasseComp}{\renewcommand*{\UPSTItitreClasseComp}{MPSI-MP}}{\newcommand*{\UPSTItitreClasseComp}{MPSI-MP}}
    \or %TSI
      \ifdef{\UPSTIidFilierePourDiagrammeCompetences}{\renewcommand{\UPSTIidFilierePourDiagrammeCompetences}{0}}{\newcommand{\UPSTIidFilierePourDiagrammeCompetences}{0}}
      \ifdef{\UPSTItitreClasseComp}{\renewcommand*{\UPSTItitreClasseComp}{TSI}}{\newcommand*{\UPSTItitreClasseComp}{TSI}}
    \or %ATS
      \ifdef{\UPSTIidFilierePourDiagrammeCompetences}{\renewcommand{\UPSTIidFilierePourDiagrammeCompetences}{0}}{\newcommand{\UPSTIidFilierePourDiagrammeCompetences}{0}}
      \ifdef{\UPSTItitreClasseComp}{\renewcommand*{\UPSTItitreClasseComp}{ATS}}{\newcommand*{\UPSTItitreClasseComp}{ATS}}
    \or %MP2I/PSI
      \ifdef{\UPSTIidFilierePourDiagrammeCompetences}{\renewcommand{\UPSTIidFilierePourDiagrammeCompetences}{1}}{\newcommand{\UPSTIidFilierePourDiagrammeCompetences}{1}}
      \ifdef{\UPSTItitreClasseComp}{\renewcommand*{\UPSTItitreClasseComp}{MP2I-PSI}}{\newcommand*{\UPSTItitreClasseComp}{MP2I-PSI}}
    \else Erreur dans UPSTIidFiliereTmp (Trop grand)
  \fi

  % Définition des angles pour les différentes filières
  % PTSI, PT, TSI, ATS
  \ifdef{\UPSTIangleCompAnalyser}{\renewcommand*{\UPSTIangleCompAnalyser}{115}}{\newcommand*{\UPSTIangleCompAnalyser}{0}}
  \ifdef{\UPSTIangleCompModeliser}{\renewcommand*{\UPSTIangleCompModeliser}{115}}{\newcommand*{\UPSTIangleCompModeliser}{0}}
  \ifdef{\UPSTIangleCompResoudre}{\renewcommand*{\UPSTIangleCompResoudre}{115}}{\newcommand*{\UPSTIangleCompResoudre}{0}}
  \ifdef{\UPSTIangleCompExperimenter}{\renewcommand*{\UPSTIangleCompExperimenter}{115}}{\newcommand*{\UPSTIangleCompExperimenter}{0}}
  \ifdef{\UPSTIangleCompCommuniquer}{\renewcommand*{\UPSTIangleCompCommuniquer}{115}}{\newcommand*{\UPSTIangleCompCommuniquer}{0}}
  \ifdef{\UPSTIangleCompConcevoir}{\renewcommand*{\UPSTIangleCompConcevoir}{115}}{\newcommand*{\UPSTIangleCompConcevoir}{0}}
  \ifdef{\UPSTIangleCompRealiser}{\renewcommand*{\UPSTIangleCompRealiser}{115}}{\newcommand*{\UPSTIangleCompRealiser}{0}}

  \ifcase\UPSTIidFilierePourDiagrammeCompetences
    % 0: PTSI, PT, TSI, ATS -> déjà définis
    \renewcommand*{\UPSTIangleCompAnalyser}{115}
    \renewcommand*{\UPSTIangleCompModeliser}{65}
    \renewcommand*{\UPSTIangleCompResoudre}{10}
    \renewcommand*{\UPSTIangleCompExperimenter}{-36}
    \renewcommand*{\UPSTIangleCompCommuniquer}{-90}
    \renewcommand*{\UPSTIangleCompConcevoir}{-144}
    \renewcommand*{\UPSTIangleCompRealiser}{170}
  \or
    % 1: MP2I, PCSI, PSI
    \renewcommand*{\UPSTIangleCompAnalyser}{120}
    \renewcommand*{\UPSTIangleCompModeliser}{60}
    \renewcommand*{\UPSTIangleCompResoudre}{0}
    \renewcommand*{\UPSTIangleCompExperimenter}{-60}
    \renewcommand*{\UPSTIangleCompCommuniquer}{-120}
    \renewcommand*{\UPSTIangleCompConcevoir}{180}
  \else
    % 2: MPSI, MP
    \renewcommand*{\UPSTIangleCompAnalyser}{90}
    \renewcommand*{\UPSTIangleCompModeliser}{14}
    \renewcommand*{\UPSTIangleCompResoudre}{-54}
    \renewcommand*{\UPSTIangleCompExperimenter}{-126}
    \renewcommand*{\UPSTIangleCompCommuniquer}{166}
  \fi

  % Dessin du diagramme
  \begin{tikzpicture}[scale=\ifstrempty{#1}{0.9}{#1}, every node/.style={scale=\ifstrempty{#1}{0.9}{#1}}]

    % Styles des boîtes actives/inactives
    \tikzstyle{boiteCompetence}=[rounded corners=2mm,draw,anchor=center,thick,minimum height=1cm,text width=2.3cm,align=center]
    \tikzstyle{actif}=[thick]
    \tikzstyle{inactif}=[dotted, thin, couleurCompetenceInactive]

    % Compétence centrale
    \node[boiteCompetence,couleurDefaultTitreClasse,fill=couleurDefaultTitreClasse!10, minimum height=1.5cm, minimum width=3.2cm] (centre) at (0,0) {\normalsize Compétences\\ \UPSTItitreClasseComp};

    % Ellipse pour le positionnement des compétences
    \begin{scope}[xscale=2]
      \coordinate (nodeAnalyser) at (\UPSTIangleCompAnalyser:\UPSTIcompetencesRayonEllipse);
      \coordinate (nodeModeliser) at (\UPSTIangleCompModeliser:\UPSTIcompetencesRayonEllipse);
      \coordinate (nodeResoudre) at (\UPSTIangleCompResoudre:\UPSTIcompetencesRayonEllipse);
      \coordinate (nodeExperimenter) at (\UPSTIangleCompExperimenter:\UPSTIcompetencesRayonEllipse);
      \coordinate (nodeCommuniquer) at (\UPSTIangleCompCommuniquer:\UPSTIcompetencesRayonEllipse);
      \coordinate (nodeConcevoir) at (\UPSTIangleCompConcevoir:\UPSTIcompetencesRayonEllipse);
      \coordinate (nodeRealiser) at (\UPSTIangleCompRealiser:\UPSTIcompetencesRayonEllipse);
    \end{scope}

    % Compétences
    \node[boiteCompetence,couleurDefaultAnalyser,\UPSTIcompetenceActifInactif{#3}] at (nodeAnalyser) {\normalsize Analyser};
    \node[boiteCompetence,couleurDefaultModeliser,\UPSTIcompetenceActifInactif{#4}] at (nodeModeliser) {\normalsize Modéliser};
    \node[boiteCompetence,couleurDefaultResoudre,\UPSTIcompetenceActifInactif{#5}] at (nodeResoudre) {\normalsize Résoudre};
    \node[boiteCompetence,couleurDefaultExperimenter,\UPSTIcompetenceActifInactif{#6}] at (nodeExperimenter) {\normalsize Expérimenter};
    \node[boiteCompetence,couleurDefaultCommuniquer,\UPSTIcompetenceActifInactif{#7}] at (nodeCommuniquer) {\normalsize Communiquer};
    \ifnumequal{1}{\UPSTIidFilierePourDiagrammeCompetences}{
      \node[boiteCompetence,couleurDefaultConcevoir,\UPSTIcompetenceActifInactif{#8}] at (nodeConcevoir) {\normalsize Concevoir};
    }{}
    \ifnumequal{0}{\UPSTIidFilierePourDiagrammeCompetences}{
      \node[boiteCompetence,couleurDefaultConcevoir,\UPSTIcompetenceActifInactif{#8}] at (nodeConcevoir) {\normalsize Concevoir};
      \node[boiteCompetence,couleurDefaultRealiser,\UPSTIcompetenceActifInactif{#9}] at (nodeRealiser) {\normalsize Réaliser};
    }{}
  \end{tikzpicture}
}

