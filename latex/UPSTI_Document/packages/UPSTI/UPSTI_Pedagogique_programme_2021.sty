\newcommand*{\UPSTIcompetencesRayonInt}{1.3}
\newcommand*{\UPSTIcompetencesRayonExt}{1.8}

% #1: échelle, #2-4: mise en valeur des écarts étudiés, #5-7: chemin des images si besoin de les changer
\newcommandx {\UPSTIdiagrammeEcarts}[7][1=,2=1,3=1,4=1,5=,6=,7=]{
  \begin{tikzpicture}[scale=\ifstrempty{#1}{1}{#1}, every node/.style={scale=\ifstrempty{#1}{1}{#1}}]

    \tikzstyle{ellipseSysteme}=[draw,ellipse,anchor=center,very thick, minimum width=3cm, minimum height=1.6cm]
    \tikzstyle{ellipseText}=[align=center,execute at begin node=\setlength{\baselineskip}{1em}]
    \tikzstyle{performances}=[align=center,execute at begin node=\setlength{\baselineskip}{1em}]
    \tikzstyle{fleche}=[>=latex,<->,line width=2pt]
    \tikzstyle{etiquette}=[execute at begin node=\setlength{\baselineskip}{1em}]

    % On grise les écarts non étudiés
    \ifnumequal{0}{#2}{\definecolor{couleurFlecheHG}{HTML}{AAAAAA}}{}
    \ifnumequal{0}{#3}{\definecolor{couleurFlecheB}{HTML}{AAAAAA}}{}
    \ifnumequal{0}{#4}{\definecolor{couleurFlecheHD}{HTML}{AAAAAA}}{}

    % Ellipse pour le positionnement des petites ellipses
    \begin{scope}[xscale=2]

      \draw[couleurSys, fill=couleurFondSys] (0,0) circle (\UPSTIcompetencesRayonInt);
      \draw[align=center] (0,0) node {Système réel\\ pluritechnologique};

      \coordinate (nodeCdc) at (90:\UPSTIcompetencesRayonExt);
      \coordinate (nodeVirtuel) at (-30:\UPSTIcompetencesRayonExt);
      \coordinate (nodeMateriel) at (-150:\UPSTIcompetencesRayonExt);

      \draw[fleche,couleurFlecheHD] (5:1.6) arc (5:55:1.6 and 2) node[etiquette,couleurFlecheHD,midway, align=left, above right=-2mm and 3mm] {\footnotesize Écart entre performances\\ \footnotesize attendues et simulées};
      \begin{scope}[xscale=-1]
        \draw[fleche,couleurFlecheHG] (5:1.6) arc (5:55:1.6 and 2) node[etiquette,couleurFlecheHG,midway, align=right, above left=-2mm and 3mm] {\footnotesize Écart entre performances\\ \footnotesize attendues et mesurées};
      \end{scope}
      \draw[fleche,couleurFlecheB] (-115:1.8) arc (-115:-65:1.8 and 3) node[etiquette,couleurFlecheB,midway, align=center, below=2mm] {\footnotesize Écart entre performances\\ \footnotesize simulées et mesurées};
    \end{scope}

    % Ellipses
    \node[ellipseSysteme, couleurSysCdc, fill=couleurFondSysCdc] at (nodeCdc) {};
    \node[ellipseSysteme, couleurSysVirtuel, fill=couleurFondSysVirtuel] at (nodeVirtuel) {};
    \node[ellipseSysteme, couleurSysMateriel, fill=couleurFondSysMateriel] at (nodeMateriel) {};
    \node[ellipseText] at (nodeCdc) {\footnotesize Cahier des\\ \footnotesize charges\\ \footnotesize Besoin};
    \node[ellipseText] at (nodeVirtuel) {\footnotesize Système\\ \footnotesize virtuel};
    \node[ellipseText] at (nodeMateriel) {\footnotesize Système\\ \footnotesize matériel};


    % Performances
    \node[performances] (perfCdc) at ($(nodeCdc)+(0,1.5)$) {\scriptsize \textbf{Performances}\\ \scriptsize \textbf{attendues}};
    \node[performances] at ($(nodeVirtuel)+(2.8,0)$) {\scriptsize \textbf{Performances}\\ \scriptsize \textbf{simulées}};
    \node[performances] at ($(nodeMateriel)+(-2.8,0)$) {\scriptsize \textbf{Performances}\\ \scriptsize \textbf{mesurées}};

    % Besoin
    \fill[couleurSymbolePower] (perfCdc)+(-3.5,0) node (nodeBesoin) {} circle (0.4);
    \draw[ultra thick] (nodeBesoin)+(65:0.25) arc (65:-245:0.25);
    \draw[ultra thick] (nodeBesoin.center) --++ (0,0.32);
    \node[performances] at ($(nodeBesoin)+(-1.5,0)$) {\scriptsize \textbf{Expression}\\ \scriptsize \textbf{du besoin}};
    \draw[fleche,couleurFlecheBesoin,->] ($(nodeBesoin)+(0.7,0)$) to[bend left] ($(nodeBesoin)+(2.1,-0.9)$);

  \end{tikzpicture}
}


%==============================================================================%
% Compétences
%==============================================================================%
% Filière (1er argument):
% 1- TSI
% 2- PTSI/PT
% 3- PCSI/PSI
% 4- MPSI/MP
% 5- ATS
% 6- MP2I/PSI
\newcommandx{\UPSTIcompetence}[3][1=,3=0]{

  % Initialisation
  \ifdef{\UPSTIcouleurCompetence}{}{\newcommand{\UPSTIcouleurCompetence}{couleurCompetenceA}}
  \ifdef{\UPSTIintituleCompetence}{}{\newcommand{\UPSTIintituleCompetence}{Compétence}}
  \ifdef{\UPSTIcompCouleur}{\renewcommand{\UPSTIcompCouleur}{}}{\newcommand{\UPSTIcompCouleur}{}}
  \ifdef{\UPSTIcompIntitule}{\renewcommand{\UPSTIcompIntitule}{}}{\newcommand{\UPSTIcompIntitule}{}}
  \ifdef{\UPSTIcompSemestre}{\renewcommand{\UPSTIcompSemestre}{}}{\newcommand{\UPSTIcompSemestre}{}}

  % Intégration avec EPBCours
  \UPSTIdetermineIdFiliere

  % Filiere (par défaut PTSI/PT - TSI - ATS) - le 1er paramètre facultatif supplante idFiliere
  \ifstrempty{#1}
  {
    \ifdef{\UPSTIidFiliereTmp}{\renewcommand{\UPSTIidFiliereTmp}{\UPSTIidFiliere}}{\newcommand{\UPSTIidFiliereTmp}{\UPSTIidFiliere}}
  }{
    \ifdef{\UPSTIidFiliereTmp}{\renewcommand{\UPSTIidFiliereTmp}{#1}}{\newcommand{\UPSTIidFiliereTmp}{#1}}
  }

  % On définit les compétences suivant la filière
  \ifcase\UPSTIidFiliereTmp
    Erreur dans le code filière (=0)

    %--------------------------------------------------------%
    % PTSI/PT
    %--------------------------------------------------------%
    \or
    \ifthenelse{\equal{#2}{A}}{\renewcommand{\UPSTIcompCouleur}{couleurCompetenceA}\renewcommand{\UPSTIcompIntitule}{Analyser}}{}
    \ifthenelse{\equal{#2}{A1}}{\renewcommand{\UPSTIcompCouleur}{couleurCompetenceA}\renewcommand{\UPSTIcompIntitule}{Analyser le besoin et les exigences}}{}
    \ifthenelse{\equal{#2}{A1-01}}{\renewcommand{\UPSTIcompCouleur}{couleurCompetenceA}\renewcommand{\UPSTIcompIntitule}{Décrire le besoin et les exigences.}\renewcommand{\UPSTIcompSemestre}{S1}}{}
